@using System.Web.Optimization
@model easygenerator.Web.SAML.IdentityProvider.Models.AssertionModel
@{
    ViewBag.Title = "Confirm service provider";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,colo=1">
    <meta name="viewport" content="width=640,maximum-scale=1.0">
    <meta name="description" content="">
    <title>Confirm service provider | easygenerator</title>
    <style type="text/css">
        @@font-face {
            font-family: 'OpenSans';
            src: url('/Content/font/opensans-regular.woff') format('woff');
            font-weight: normal;
            font-style: normal;
        }

        html, body {
            margin: 0;
            padding: 0;
            background-color: #f3f7f9;
        }

        .eg-header {
            height: 50px;
            background-color: #2f353e;
        }

        .eg-logo {
            height: 28px;
            width: 152px;
            display: inline-block;
            margin: 11px;
            background-image: url('data:image/png;base64,');
            background-repeat: no-repeat;
        }

            .eg-logo:hover {
                background-image: url('data:image/png;base64,');
            }

        .confirm-popup-container {
            min-width: 638px;
            max-width: 900px;
            margin: 20px auto 0;
            box-shadow: 0 1px 1px 1px #dadee0;
            border-radius: 3px;
            background-color: #fff;
            background-image: url('data:image/png;base64,');
            background-position: -232px 96px;
            background-repeat: no-repeat;
        }

        .confirm-popup {
            padding: 0 40px;
        }

        .confirm-popup-header {
            padding: 40px 0 16px 0;
        }

        .confirm-popup-title {
            margin: 0;
            color: #42515f;
            font: 22px 'OpenSans';
        }

        .service-url {
            color: #42515f;
            font: 13px 'OpenSans';
            transition: all 0.2s;
        }

        .service-url:hover {
            color: #469dd5;
        }

        .confirm-user-info-title {
            color: #42515f;
            font: 13px 'OpenSans';
        }

        .user-info-list {
            margin-top: 30px;
            list-style: none;
            padding: 0;
        }

        .user-info-list-item {
            margin-bottom: 20px;
        }

        .user-info-list-item:before {
            content: '';
            display: inline-block;
            vertical-align: middle;
            width: 31px;
            height: 32px;
            margin-right: 8px;
            background-image: url('data:image/png;base64,');
            background-repeat: no-repeat;
        }

        .user-info-item-name {
            display: inline-block;
            vertical-align: middle;
            color: #63798e;
            font: 13px 'OpenSans';
        }

        .confirm-popup-footer {
            margin-top: 50px;
            padding: 20px 0;
            text-align: right;
            border-top: 1px solid #e2e8eb;
        }

        .deny-action {
            min-width: 82px;
            margin-right: 16px;
            color: #738699;
            background-color: #fff;
            box-shadow: 0 0 0 1px #91a1b0;
        }

        .deny-action:hover {
            color: #63798e;
            box-shadow: 0 0 0 1px #63798e;
        }

        .allow-action {
            min-width: 82px;
            color: #fff;
            background-color: #4caf50;
            box-shadow: 0 1px 3px 0 #b3b3b3;
        }

        .allow-action:hover {
            box-shadow: 0 2px 5px 0 #b3b3b3;
        }

        .btn {
            padding: 8px;
            font: 13px 'OpenSans';
            border-radius: 3px;
            border: none;
            cursor: pointer;
            transition: all 0.2s;
            outline: none;
        }
    </style>
</head>
<body>
    <header class="eg-header">
        <a href="/#" class="eg-logo"></a>
    </header>
    <section class="confirm-popup-container">
        <div class="confirm-popup">
            <header class="confirm-popup-header">
                <h1 class="confirm-popup-title">The following service is trying to use your Easygenerator account info to login</h1>
                <p class="service-url-container">
                    <a class="service-url" href="@Model.AssertionConsumerServiceUrl" target="_blank">@Model.AssertionConsumerServiceUrl</a>
                </p>
            </header>
            <section class="confirm-user-info-section">
                <h2 class="confirm-user-info-title">Would you like to allow this service to access some of your Easygenerator info:</h2>
                <ul class="user-info-list">
                    <li class="user-info-list-item">
                        <span class="user-info-item-name">Your email address</span>
                    </li>
                    <li class="user-info-list-item">
                        <span class="user-info-item-name">First and Last name</span>
                    </li>
                </ul>
            </section>
            <footer class="confirm-popup-footer">
                <button class="deny-action btn" type="button" id="deny">Deny</button>
                <button class="allow-action btn" type="button" id="allow">Allow</button>
            </footer>
        </div>
    </section>

    <div id="crossProtocolStorageFrames"></div>

    @Scripts.Render("~/bundless/scripts/idp/auth")

    <script type="text/javascript">
        function deny() {
            window.location.href = '/';
        }

        function allow() {
            window.auth.getHeader('auth').then(function (value) {
                var headers = value;
                headers["cache-control"] = "no-cache";

                $.ajax({
                    method: 'POST',
                    url: '/saml/idp/SignIn',
                    headers: headers,
                    data: {
                        InResponseTo: '@Model.InResponseTo',
                        AssertionConsumerServiceUrl: '@Model.AssertionConsumerServiceUrl',
                        RelayState: '@Model.RelayState',
                        Audience: '@Model.Audience',
                        Uid: '@Model.Uid'
                    }
                }).then(function (data) {
                    if (!data) {
                        deny();
                    }
                    var $relayStateField = $('<input type="hidden" name="RelayState" value="' + data.RelayState + '"/>');
                    var $samlResponseField = $('<input type="hidden" name="SAMLResponse" value="' + data.SAMLResponse + '"/>');
                    var $form = $('<form action="' + data.DestinationUrl + '" method="post"></form>');
                    $form.append($relayStateField);
                    $form.append($samlResponseField);
                    if (data.uid) {
                        $form.append($('<input type="hidden" name="uid" value="' + data.uid + '"/>'));
                    }
                    $('body').append($form);
                    $form.submit();
                }).fail(function () {
                    deny();
                });
            });
        }

        $('#deny').bind('click', function () {
            deny();
        });
        $('#allow').bind('click', function () {
            allow();
        });
    </script>
</body>
</html>
