@model ICollection<string>
@{
    ViewBag.Title = "Objectives";
}

<h2>Objectives</h2>



@if (Model.Count > 0)
{
    <ul>
        @foreach (var item in Model)
        {
            <li>@item</li>
        }
    </ul>
    @Html.ActionLink("Create Objective", "Create")
}
else
{
    <div>Click @Html.ActionLink("here", "Create") to create your first objective</div>
}

